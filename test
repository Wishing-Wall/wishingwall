#!/usr/bin/env bash

set -e

if [ ! -f install ]; then
    echo 'install must be run within its container folder' 1>&2
    exit 1
fi

CURDIR=`pwd`
OLDGOPATH="$GOPATH"
export GOPATH="$CURDIR:$CURDIR/thirdparty"

gofmt -w src

#go test logger
#go test db
go test rpc
#go test protocol
export GOPATH="$OLDGOPATH"
export PATH="$OLDPATH"

echo 'finished'
echo 'test'
