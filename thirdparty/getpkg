#!/usr/bin/env bash

set -e

if [ ! -f getpkg ]; then
    echo 'getpkg must be run within its container folder' 1>&2
    exit 1
fi

OLDGOPATH="$GOPATH"
export GOPATH=`pwd`

#go get -u github.com/go-martini/martini
#go get -u github.com/codegangsta/martini-contrib/auth
#go get -u github.com/philsong/goleveldb/leveldb
#go get -u github.com/Wishing-Wall/btcrpcclient
go get -u github.com/Wishing-Wall/go-sqlite3
export GOPATH="$OLDGOPATH"
export PATH="$OLDPATH"

echo 'finished'
